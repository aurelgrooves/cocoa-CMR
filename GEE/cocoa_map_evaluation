// Load probability + aleatoric uncertainty layers
var agroProb = ee.Image("projects/ee-cocoacmr/assets/outputs/CMR_agroforestryCocoaProb_2020_v1_1")
              .select('p_mean').rename('agroProb');
var agroAle  = ee.Image("projects/ee-cocoacmr/assets/outputs/CMR_agroforestryCocoaProb_2020_v1_1")
              .select('ale').rename('agroAle');

var openProb = ee.Image("projects/ee-cocoacmr/assets/outputs/CMR_fullSunCocoaProb_2020_v1_1")
              .select('p_mean').rename('openProb');
var openAle  = ee.Image("projects/ee-cocoacmr/assets/outputs/CMR_fullSunCocoaProb_2020_v1_1")
              .select('ale').rename('openAle');

// Palettes
var probPalette = ['yellow','orange','red','purple','black'];
var alePalette  = palettes.colorbrewer.RdYlGn[11];

// Reference layer previews
Map.layers().add(ui.Map.Layer(agroProb.selfMask(), {min:0,max:100,palette:probPalette}, 'Agroforestry probability'),false);
Map.layers().add(ui.Map.Layer(openProb.selfMask(), {min:0,max:100,palette:probPalette}, 'Open canopy probability', false));
Map.layers().add(ui.Map.Layer(agroAle.selfMask(), {min:0,max:25,palette:alePalette}, 'Agroforestry uncertainty', false));
Map.layers().add(ui.Map.Layer(openAle.selfMask(), {min:0,max:25,palette:alePalette}, 'Open canopy uncertainty', false));

// Legend function
function makeLegend(title, palette, min, max) {
  var p = ui.Panel({style:{padding:'6px', backgroundColor:'rgba(255,255,255,0.9)'}});
  p.add(ui.Label({value:title, style:{fontWeight:'bold', fontSize:'12px', margin:'0 0 4px 0'}}));
  
  var grad = ui.Thumbnail({
    image: ee.Image.pixelLonLat().select('latitude').multiply(0)
      .add(ee.Image.pixelLonLat().select('longitude'))
      .visualize({min:0,max:1,palette:palette}),
    params:{bbox:'0,0,1,0.1', dimensions:'160x18'},
    style:{margin:'4px 0'}
  });
  
  p.add(grad);
  p.add(ui.Label(min + ' â†’ ' + max, {fontSize:'10px', margin:'0'}));
  return p;
}

// Legend box
var legendBox = ui.Panel({
  layout: ui.Panel.Layout.flow('vertical'),
  style:{position:'bottom-right', padding:'4px'}
});
legendBox.add(makeLegend('Probability (%)', probPalette, 0, 100));
legendBox.add(makeLegend('Uncertainty (aleatoric)', alePalette, 0, 25));
ui.root.add(legendBox);

// Area reducer
function areaInt(img, band, geom){
  return ee.Number(
    img.multiply(ee.Image.pixelArea())
      .reduceRegion({
        reducer: ee.Reducer.sum(),
        geometry: geom,
        scale: 100,
        maxPixels: 1e13
      })
      .get(band)
  ).divide(10000).round();
}

// ---------------- Masks ----------------
var openCocoa = openProb.gt(openThreshold)
  .and(openAle.lt(openAleThreshold))
  .rename('openCocoa');

var agroCocoa = agroProb.gt(agroThreshold)
  .and(agroAle.lt(agroAleThreshold))
  .and(openCocoa.eq(0))
  .rename('agroCocoa');

var totalCocoa = ee.Image(0)
  .where(openCocoa.eq(1), 1)
  .where(agroCocoa.eq(1), 2)
  .updateMask(F)
  .rename('totalCocoa')
  .selfMask();

// ---------------- Area summaries ----------------
var openAreaEE  = areaInt(openCocoa,  'openCocoa',  aoi);
var agroAreaEE  = areaInt(agroCocoa,  'agroCocoa',  aoi);
var totalAreaEE = areaInt(totalCocoa, 'totalCocoa', aoi);

print('Open cocoa ha', openAreaEE);
print('Agro cocoa ha', agroAreaEE);
print('Total cocoa ha (no overlap)', totalAreaEE);

// ---------------- Map masks with dynamic names ----------------
Map.addLayer(
  openCocoa.selfMask(),
  {palette:['orange']},
  'Open cocoa mask (p>' + openThreshold + ', ale<' + openAleThreshold + ')'
);

Map.addLayer(
  agroCocoa.selfMask(),
  {palette:['#755b14']},
  'Agro cocoa mask (p>' + agroThreshold + ', ale<' + agroAleThreshold + ')'
);

Map.addLayer(
  totalCocoa.selfMask(),
  {palette:['yellow','brown']},
  'Combined cocoa classes'
);

