            ee.Geometry.Point([8.908973267480652, 4.960871171950371]),
            {
              "Ref_FNFW": 2,
              "system:index": "111"
            })]),
    GFCparam = {"opacity":1,"bands":["last_b50","last_b40","last_b30"],"min":13,"max":94,"gamma":1},
    GHSL = ee.ImageCollection("JRC/GHSL/P2023A/GHS_BUILT_S_10m"),
    geometry = 
    /* color: #d63000 */
    /* shown: false */
    ee.Geometry.Polygon(
        [[[10.091872829060469, 6.574017080957755],
          [10.100112575154219, 5.9542685464929725],
          [11.224837916951094, 5.946073192649689],
          [11.222091334919844, 6.574017080957755]]], null, false),
    alosParam = {"opacity":1,"bands":["HH_median","HV_median","HV_median"],"min":-31.379663351224092,"max":-1.32036391184684,"gamma":1},
    alos2 = ee.Image("projects/ee-cocoacmr/assets/imagery/CMR_ALOS2_2020"),
    S2 = ee.Image("projects/ee-cocoacmr/assets/imagery/CMR_S2_2020"),
    planet = ee.Image("projects/ee-cocoacmr/assets/imagery/planet_mosaic_2020"),
    S1 = ee.Image("projects/ee-cocoacmr/assets/imagery/CMR_S1_2020"),
    JRC_FNF_2020 = ee.ImageCollection("JRC/GFC2020/V2");
/***** End of imports. If edited, may not auto-convert in the playground. *****/
// ---------------- TRAINING POINTS ----------------
var FsourcesPts   = ee.FeatureCollection('projects/ee-cocoacmr/assets/training_data/all_sources_Ftraining_CMR');
var NFsourcesPts  = ee.FeatureCollection('projects/ee-cocoacmr/assets/training_data/all_sources_NFtraining_CMR');
var GEDIPts       = ee.FeatureCollection('projects/ee-cocoacmr/assets/training_data/GEDI_FNF_training_CMR');

var CAFIFPts = ee.FeatureCollection('projects/ee-cocoacmr/assets/training_data/CMR_CAFI_2015_2022_2023_stable')
                  .filter(ee.Filter.neq('Ref_FNF_2015', 1))
                  .randomColumn();
                  
var CAFINFPts = ee.FeatureCollection('projects/ee-cocoacmr/assets/training_data/CMR_CAFI_2015_2022_2023_stable')
                  .filter(ee.Filter.neq('Ref_FNF_2015', 0))
                  .randomColumn();

var CAFIPts_sel = CAFIPts.filter(ee.Filter.lte('random', 0.25));

var trainingPts = NFsourcesPts
  .merge(FsourcesPts)
  .merge(GEDIPts)
  .merge(add_forest_pts)
  .merge(add_nf_points)
  .merge(add_water_pts)
  .merge(add_plantations_agro_pts);

Map.addLayer(trainingPts.filter(ee.Filter.eq('Ref_FNFW', 1)), {color:'green'}, 'Forest', false);
Map.addLayer(trainingPts.filter(ee.Filter.eq('Ref_FNFW', 2)), {color:'grey'},  'Non-Forest', false);
Map.addLayer(trainingPts.filter(ee.Filter.eq('Ref_FNFW', 3)), {color:'blue'},  'Water', false);

// Assign class
var forest    = trainingPts.filter('Ref_FNFW == 1').randomColumn('rnd').map(function(f){return f.set('class',1)});
var nonForest = trainingPts.filter('Ref_FNFW > 1').randomColumn('rnd').map(function(f){return f.set('class',0)});

var forestTrain = forest.filter('rnd > 0.2');
var forestVal   = forest.filter('rnd <= 0.2');

var nonForestTrain = nonForest.filter('rnd > 0.2');
var nonForestVal   = nonForest.filter('rnd <= 0.2');

var nTrain = nonForestTrain.size();
var nVal   = nonForestVal.size();

var training   = forestTrain.sort('rnd').limit(nTrain).merge(nonForestTrain);
var validation = forestVal.sort('rnd').limit(nVal).merge(nonForestVal);

print('training size',   training.aggregate_histogram('class'));
print('validation size', validation.aggregate_histogram('class'));

Export.table.toAsset({
  collection: training,
  description: 'training_data',
  assetId: 'projects/ee-cocoacmr/assets/training_data/TNTforestTrain'
});

Export.table.toAsset({
  collection: validation,
  description: 'validation_data',
  assetId: 'projects/ee-cocoacmr/assets/training_data/TNTforestVal'
});
